# Figure parameters
figure.figsize: 12, 5
figure.dpi: 80
figure.edgecolor: 1

# Axes parameters
axes.linewidth: 0.8
axes.titlesize: 18
axes.labelsize: 16
axes.grid: True
axes.prop_cycle: cycler('color', ['1f77b4', 'ff7f0e', '2ca02c', 'd62728', '9467bd', '8c564b', 'e377c2', '7f7f7f', 'bcbd22', '17becf'])

# Ticks
xtick.labelsize:14
ytick.labelsize:14

# Grid parameters
grid.linewidth:0.5
grid.alpha: 0.5

lines.linewidth:2
lines.markersize: 3

# Legend parameters
legend.fontsize: 10
legend.title_fontsize: 14
legend.shadow: False

